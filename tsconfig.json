{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./built"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    "coverage",
    "jest.config.js",
    "__test__",
    "./built/**/*"
  ],
}
